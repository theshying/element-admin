{
  "name": "element-admin-pro",
  "version": "0.0.1",
  "description": "开箱即用的vue+element 中后台系统开发框架",
  "author": "theShy <969153746@qq.com>",
  "scriptsComments": {
    "dev": "启动前端开发服务和mock-server，监听前端资源变更并进行打包",
    "preview": "项目预览",
    "build": "生产打包",
    "helper:build:font": "生成图标样式",
    "helper:theme:init": "生成自定义主题变量文件，请勿重复生成",
    "helper:theme:dev": "监听自定义主题变量文件修改并生成样式文件",
    "helper:theme:build": "编译自定义主题生成样式文件",
    "lint": "检查 eslint 规则",
    "lint:fix": "按照 eslint 规则自动修复"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "preview": "node build/index.js --preview",
    "build": "vue-cli-service build",
    "helper:build-font": "node -e \"require('./build/generateIcon.js').build()\"",
    "helper:theme:init": "et --init",
    "helper:theme:dev": "et --watch ",
    "helper:theme:build": "et --minimize",
    "lint": "eslint --ext .js,.vue src ",
    "lint:fix": "eslint --ext .js,.vue src  --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.6.9",
    "dayjs": "^1.8.16",
    "element-ui": "^2.12.0",
    "nprogress": "^0.2.0",
    "qs": "^6.9.0",
    "vue": "^2.6.10",
    "vue-i18n": "^7.8.1",
    "vue-router": "3.0.7",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-eslint": "^3.9.1",
    "@vue/cli-plugin-unit-jest": "3.5.3",
    "@vue/cli-service": "3.5.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "connect": "3.6.6",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "8.1.5",
    "mockjs": "1.0.1-beta3",
    "node-sass": "^4.9.0",
    "plop": "2.3.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "runjs": "^4.3.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1.13.2",
    "vue-template-compiler": "2.6.10",
    "webpack-iconfont-plugin-nodejs": "^1.0.16"
  },
  "optionalDependencies": {
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.4.9"
  },
  "element-theme": {
    "browsers": [
      "> 1%",
      "not ie <= 8"
    ],
    "out": "./src/assets/style/element-theme-chalk/",
    "config": "./src/assets/style/element-variables.scss",
    "theme": "element-theme-chalk"
  },
  "engines": {
    "node": ">= 8.9.4",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theshying/element-admin-pro/issues"
  },
  "homepage": "https://github.com/theshying/element-admin-pro#readme"
}
