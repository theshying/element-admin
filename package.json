{
  "name": "element-admin-pro",
  "version": "0.0.1",
  "description": "开箱即用的vue+element 中后台系统开发框架",
  "author": "theShy <969153746@qq.com>",
  "scriptsComments": {
    "dev": "启动前端开发服务，监听前端资源变更并进行打包，且提供前端 web 服务",
    "dev:mock": "启动前端开发服务，监听前端资源变更并进行打包，且提供前端 web 服务，API 数据从 mock 服务获取",
    "dev:watch": "启动前端开发服务，监听前端资源变更并进行打包",
    "test": "运行前端测试",
    "test:unit": "运行单元测试",
    "build": "生产打包",
    "helper:build:font": "生成图标样式",
    "helper:theme:init": "生成自定义主题变量文件，请勿重复生成",
    "helper:theme:dev": "监听自定义主题变量文件修改并生成样式文件",
    "helper:theme:build": "编译自定义主题生成样式文件",
    "helper:i18n:json": "将多语言文件生成json文件供后端调用",
    "lint": "检查 eslint 规则",
    "lint:fix": "按照 eslint 规则自动修复"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 DEV_MODE=hot webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:mock": "concurrently \"npm run helper:server:mock\" \"cross-env DEV_ENV=mock npm run dev\"",
    "dev:watch": "cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack --config build/webpack.dev.conf.js --progress -d --watch",
    "test": "npm run test:unit",
    "test:unit": "jest --config test/unit/jest.conf.js --coverage",
    "build": "node --max_old_space_size=4096 build/build.js",
    "helper:build-font": "node -e \"require('./build/generateIcon.js').build()\"",
    "helper:server:mock": "node build/mock-server.js",
    "helper:theme:init": "et --init",
    "helper:theme:dev": "et --watch ",
    "helper:theme:build": "et --minimize",
    "helper:i18n.transform": "node build/transform.js",
    "lint": "eslint --ext .js,.vue src test/unit",
    "lint:fix": "eslint --ext .js,.vue src test/unit --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
